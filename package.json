{
  "name": "kubi",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun dev:up && bun db:migrate && bun locales:compile && bun front:dev",
    "dev:up": "bun run ./scripts/dev/up.ts",
    "dev:down": "bun run ./scripts/dev/down.ts",
    "dev:logs": "bun run ./scripts/dev/logs.ts",
    "dev:rm": "bun run ./scripts/dev/rm.ts",
    "build": "bun locales:compile && bun front:build",
    "front:dev": "bunx --bun vinxi dev",
    "front:build": "bunx --bun vinxi build",
    "front:start": "bunx --bun vinxi start",
    "start": "bun db:migrate && bun front:start",
    "check:types": "bun locales:compile && tsc --noEmit",
    "lint": "eslint . --fix",
    "check:lint": "eslint .",
    "check:deadcode": "bun locales:compile && dotenv -e .env.ci -- knip-bun",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun db:generate && bun run ./app/db/migrate.ts",
    "ui": "bunx shadcn@latest",
    "pwa:assets": "pwa-assets-generator",
    "locales:extract": "lingui extract --clean",
    "locales:compile": "lingui compile",
    "locales:build": "bun locales:extract && bun locales:compile",
    "check:locales": "bun locales:build --strict",
    "web:build": "nixpacks build . -o ."
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.15.1",
    "@eslint/js": "^9.13.0",
    "@lingui/cli": "^4.13.0",
    "@lingui/conf": "^4.13.0",
    "@lingui/vite-plugin": "^4.13.0",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@tanstack/eslint-plugin-router": "^1.74.5",
    "@types/bun": "latest",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lingui": "^0.6.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "knip": "^5.34.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite-plugin-pwa": "^0.20.5",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^10.1.0",
    "@lingui/core": "^4.13.0",
    "@lingui/macro": "^4.13.0",
    "@lingui/react": "^4.13.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-three/drei": "^9.115.0",
    "@react-three/fiber": "^8.17.10",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.59.16",
    "@tanstack/react-router": "^1.77.5",
    "@tanstack/react-router-with-query": "^1.77.5",
    "@tanstack/router-devtools": "^1.77.5",
    "@tanstack/start": "^1.77.5",
    "@vitejs/plugin-react": "^4.3.3",
    "arctic": "^2.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "lucia": "^3.2.2",
    "lucide-react": "^0.454.0",
    "postgres": "^3.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-qr-barcode-scanner": "^2.0.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.169.0",
