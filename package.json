{
  "name": "kubi",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun dev:up && bun db:migrate && bun locales:compile && bun front:dev",
    "dev:up": "bun run ./scripts/dev/up.ts",
    "dev:down": "bun run ./scripts/dev/down.ts",
    "dev:logs": "bun run ./scripts/dev/logs.ts",
    "dev:rm": "bun run ./scripts/dev/rm.ts",
    "build": "bun locales:compile && bun front:build",
    "front:dev": "bunx --bun vinxi dev",
    "front:build": "bunx --bun vinxi build",
    "front:start": "bunx --bun vinxi start",
    "start": "bun db:migrate && bun front:start",
    "check:types": "bun locales:compile && tsc --noEmit",
    "lint": "eslint . --fix",
    "check:lint": "eslint .",
    "check:deadcode": "bun locales:compile && dotenv -e .env.ci -- knip-bun",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun db:generate && bun run ./app/db/migrate.ts",
    "ui": "bunx shadcn@latest",
    "pwa:assets": "pwa-assets-generator",
    "locales:extract": "lingui extract --clean",
    "locales:compile": "lingui compile",
    "locales:build": "bun locales:extract && bun locales:compile",
    "check:locales": "bun locales:build --strict",
    "web:build": "nixpacks build . -o ."
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.21.0",
    "@eslint/js": "^9.17.0",
    "@lingui/cli": "^5.1.2",
    "@lingui/conf": "^5.1.2",
    "@lingui/vite-plugin": "^5.1.2",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@tanstack/eslint-plugin-router": "^1.87.6",
    "@types/bun": "latest",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vite-pwa/assets-generator": "^0.2.6",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "dotenv-cli": "^7.4.4",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lingui": "^0.8.4",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "knip": "^5.41.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^10.2.0",
    "@lingui/core": "^5.1.2",
    "@lingui/react": "^5.1.2",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-query-devtools": "^5.62.8",
    "@tanstack/react-router": "^1.91.3",
    "@tanstack/react-router-with-query": "^1.91.3",
    "@tanstack/router-devtools": "^1.91.3",
    "@tanstack/start": "^1.91.3",
    "@vitejs/plugin-react": "^4.3.4",
    "arctic": "^2.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "lucia": "^3.2.2",
    "lucide-react": "^0.468.0",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "^0.5.1",
    "zod": "^3.24.1"
  },
  "patchedDependencies": {
    "knip@5.34.1": "patches/knip@5.34.1.patch"
  }
}