    "@supabase/supabase-js": "^2.49.4",
    "@types/bun": "^1.2.9",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "autoprefixer": "^10.4.21",
    "babel-plugin-macros": "^3.1.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.6",
    "knip": "^5.50.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
