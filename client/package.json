		"dotenv-cli": "^8.0.0",
		"knip": "^5.60.2",
		"postcss": "^8.5.4",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.8.3",
		"vite-plugin-pwa": "^1.0.0",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"dependencies": {
		"@dair/common": "workspace:*",
