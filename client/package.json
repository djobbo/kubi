{
	"name": "@dair/client",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun locales:compile && bunx --bun vinxi dev --host",
		"build": "bun panda:build && bun locales:compile && bunx --bun vinxi build",
		"start": "bunx --bun vinxi start --host",
		"check:types": "bun locales:compile && tsc --noEmit",
		"lint": "biome check --write",
		"check:lint": "biome check",
		"ui": "bunx --bun shadcn@latest",
		"pwa:assets": "pwa-assets-generator",
		"locales:extract": "lingui extract --clean",
		"locales:compile": "lingui compile",
		"locales:build": "bun locales:extract && bun locales:compile",
		"check:locales": "bun locales:build --strict",
		"prepare": "[ \"$NODE_ENV\" != \"production\" ] && bun panda:build || exit 0",
		"panda:build": "panda codegen",
		"migration:start": "bun run ./scripts/migration/migrate.ts"
	},
	"devDependencies": {
		"@dair/api": "workspace:*",
		"@lingui/cli": "^5.3.2",
